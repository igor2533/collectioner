{% extends 'base.html.twig' %}
{% block body %}

<div class="container mons">

    {%  for item in items %}
  <div class="col-sm-3">

      <div class="col-sm-12"><a href="/item/{{ item.slug }}"> <img src="/uploads/{{ item.image }}"/></a></div>
      <div class="col-sm-12"><span> {{ item.title }}</span></div>

  </div>
    {%  endfor %}
</div>


{% endblock %}